    "#decorates\n",
    "def factorial(n):\n",
    "    if(n<1):\n",
    "        return 1\n",
    "    else:\n",
    "        return n*factorial(n-1)\n",
    "# a = factorial\n",
    "def operators(fun, x):\n",
    "    result = fun(x)\n",
    "    return result\n",
    "operators(factorial, 5)"
